#INCLUDE "rwmake.ch"

USER FUNCTION RDFAT049()

Local lRet:= .T. 

IF(M->ZF_PRECO > M->ZF_PRCMAX)
	lRet:= .F.
	MSGALERT("PREùO DE VENDA DEVE SER MENOR OU IGUAL AO PREùO MùXIMO!")
	
ENDIF

IF(M->ZF_PRECO < M->ZF_PRCMIN)
	lRet:= .F.
	MSGALERT("PREùO DE VENDA DEVE SER MAIOR OU IGUAL AO PREùO MùNIMO!")
	
ENDIF	

IF(M->ZF_PRCMIN > M->ZF_PRCMAX)
	lRet:= .F.
	MSGALERT("PRECO MùNIMO NAO PODES SER MAIOR QUE PREùO MùXIMO")

ENDIF

RETURN lRet